Table: Cars
- ID Primary Key | NOT NULL | UNIQUE

- Marca - VARCHAR(12)

- Modello - VARCHAR(12) | NOT NULL

- Cilindrata - MEDIUMINT

- Cavalli - SMALLINT

- Trazione - VARCHAR(12)

- Carburante - VARCHAR(12)

- Numero posti abilitati - TINYINT

